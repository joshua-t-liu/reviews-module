{
  "name": "adidas-reviews",
  "description": "FEC Reviews module",
  "author": "alexzhaohong <alexhong96@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=6.13.0"
  },
  "scripts": {
    "start": "node ./server/index.js",
    "server-dev": "open http://localhost:3003 && nodemon ./server/index.js ",
    "build:bundle": "webpack -d",
    "react-dev": "webpack -d --watch",
    "test": "jest",
    "test:init": "jest --init",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "build": "babel ./client/src/ --out-dir compiled --presets @babel/preset-react --source-maps inline --watch",
    "seed": "mysql -u root -p < schema.sql",
    "build:docker": "./docker_build.sh"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "newrelic": "^6.8.0",
    "pg": "^8.2.1",
    "redis": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/polyfill": "^7.10.1",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "axios": "^0.19.2",
    "babel-jest": "^25.5.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "chai": "^4.2.0",
    "cors": "^2.8.5",
    "css-loader": "^3.5.3",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-hackreactor": "git://github.com/reactorcore/eslint-config-hackreactor",
    "faker": "^4.1.0",
    "jest": "^25.5.4",
    "morgan": "^1.10.0",
    "node-fetch": "^2.6.0",
    "nodemon": "^2.0.3",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "sinon": "^9.0.2",
    "style-loader": "^1.2.1",
    "styled-components": "^5.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "babel": {
    "presets": [
      "@babel/env",
      "@babel/react"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": false,
          "fileName": false
        }
      ]
    ]
  },
  "version": "1.0.0",
  "main": "server/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexzhaohong/adidas-reviews.git"
  },
  "keywords": [
    "fec",
    "hrsf"
  ],
  "bugs": {
